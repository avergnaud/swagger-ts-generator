{
  "name": "swagger-ts-generator",
  "version": "1.1.29",
  "description": "Given a swagger.json file, generates a number of TypeScript files which can be used as models and model-driven forms in Angular 2 (and above)",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "npm run build -- --watch",
    "publish-patch": "npx babel  build --out-dir built && npm version patch && npm publish",
    "test": "./test/updateSwagger.js"
  },
  "keywords": [
    "Swagger",
    "Code Generator",
    "models",
    "model-driven forms",
    "reactive forms"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/avergnaud/swagger-ts-generator"
  },
  "author": "A.Vergnaud & Y.Serra forked thanks to  areijngoudt@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.111",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "fs": "0.0.2",
    "handlebars": "^4.0.11",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "request": "^2.83.0",
    "typescript": "^2.9.2",
    "vinyl-source-stream": "^1.1.2",
    "yargs": "^4.6.0"
  }
}
